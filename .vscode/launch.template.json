  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "OrchestratorV3",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "internalConsoleOptions": "openOnSessionStart",
      "program": "${workspaceFolder}/Tasks/OrchestratorV3/task.ts",
      "outFiles": [
        "${workspaceFolder}/**/*.js"
      ],
      "env": {
        "SYSTEM_DEBUG": "true",
        "DEBUG": "true"
      }
    }
  ]
}
